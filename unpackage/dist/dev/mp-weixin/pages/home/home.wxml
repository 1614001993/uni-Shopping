<view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}"><block wx:for="{{swipperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><navigator class="swiper-item" url="{{'/subpkg/goods-detail/goods-detail?goods_id='+item.goods_id}}"><image class="swiperImg" src="{{item.image_src}}" mode></image></navigator></swiper-item></block></swiper><view class="nav-list"><block wx:for="{{categoryNav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navClickhandler',['$0'],[[['categoryNav','',index]]]]]]]}}" class="nav-item" bindtap="__e"><image class="nav-img" src="{{item.image_src}}"></image></view></block></view><view class="floor-list"><block wx:for="{{FloorList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="floor-item"><image class="floor-title" src="{{item.floor_title.image_src}}" mode></image><view class="floor-img-box"><navigator class="left-img-box" url="{{item.product_list[0].url}}"><image style="{{'width:'+(item.product_list[0].image_width+'rpx')+';'}}" src="{{item.product_list[0].image_src}}" mode="widthFix"></image></navigator><view class="right-img-box"><block wx:for="{{item.product_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index!==0}}"><navigator class="right-img-item" url="{{item.url}}"><image style="{{'width:'+(item.image_width+'rpx')+';'}}" src="{{item.image_src}}" mode="widthFix"></image></navigator></block></block></view></view></view></block></view></view>